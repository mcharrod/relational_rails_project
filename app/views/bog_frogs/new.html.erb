<%= form_with url: "/bogs/#{@bog.id}/frogs", method: :post, local: true do |form| %>
  <p><%= form.label :name, "Frog name" %></p>
  <p><%= form.text_field :name %></p>
  <p><%= form.label :age, "Frog age" %></p>
  <p><%= form.text_field :age %></p>
  <p><%= form.label :singing %></p>
  <p><%= form.select :singing, options_for_select([true, false]), class: 'form-control' %></p>
  <p><%= form.submit %></p>
<% end %>
